{
  "name": "svelte-grid-responsive",
  "version": "0.3.0",
  "description": "Responsive grid system based on Bootstrap for Svelte",
  "repository": "https://github.com/andrelmlins/svelte-grid-responsive",
  "author": "Andr√© Lins <andrelucas01@hotmail.com> (https://andrelmlins.github.io/)",
  "license": "MIT",
  "svelte": "src/lib/Grid.svelte",
  "module": "dist/index.mjs",
  "main": "dist/index.js",
  "homepage": "https://svelte-grid-responsive.netlify.com/",
  "private": false,
  "files": [
    "src/lib",
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public",
    "test": "jest src/lib",
    "coverage": "npm run test --coverage"
  },
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@babel/preset-env": "^7.11.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/svelte": "^3.0.0",
    "babel-jest": "^26.3.0",
    "jest": "^26.4.2",
    "rollup": "^2.26.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-svelte": "^6.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "sirv-cli": "^1.0.0",
    "svelte": "^3.24.0",
    "svelte-jester": "^1.1.2"
  },
  "bugs": {
    "url": "https://github.com/andrelmlins/svelte-grid-responsive/issues"
  },
  "keywords": [
    "svelte",
    "grid",
    "responsive",
    "bootstrap"
  ],
  "jest": {
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.svelte$": [
        "svelte-jester",
        {
          "preprocess": false,
          "debug": false,
          "compilerOptions": {},
          "rootMode": ""
        }
      ]
    },
    "moduleFileExtensions": [
      "js",
      "svelte"
    ]
  }
}
